<?xml version="1.0" encoding="UTF-8"?>

<Configuration package="fr.humanbooster.fx.kanban_ws" status="INFO">
        <Appenders>
                <File name="File" fileName="/root/eclipse-workspace/kanbanWSComplet/log/kanban_log">
                        <PatternLayout>
                                <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
                        </PatternLayout>
                </File>
        </Appenders>
        <Loggers>
                <Logger name="fr.humanbooster.fx.kanban_ws.controller.KanbanRestController" level="error">
                        <AppenderRef ref="File" />
                </Logger>
                <Root level="info">
                        <AppenderRef ref="File" />
                </Root>
        </Loggers>
</Configuration>